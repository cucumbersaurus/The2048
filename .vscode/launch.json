{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "(gdb) 시작",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}\\main.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "C:\\MinGW\\bin",
            "environment": [],
